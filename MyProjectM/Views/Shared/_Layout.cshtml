<body>
<!-- header -->
	<div class="header">
		<div class="container">
			

					<div class=" w3layouts_logo">
						<a asp-area="" asp-controller="Home" asp-action="Index"><h1>One<span>Movies</span></h1></a>
					</div>
					<div class="w3_search">
						<form action="#" method="get">
							<input type="text" name="Search" placeholder="Search" required="">
							<input type="submit" value="Go">
						</form>
					</div>
					<div class="w3l_sign_in_register">
						<ul>
							<li><i class="fa fa-phone" aria-hidden="true"></i> (+000) 123 345 653</li>
						<partial name="_LoginPartial.cshtml" />
			
						</ul>
					</div>
			
			<div class="clearfix"> </div>
		</div>
	</div>
<!-- //header -->
<!-- bootstrap-pop-up -->

<!-- //bootstrap-pop-up -->
<!-- nav -->
	<style>
		 a.navbar-brand {
            float: center;
            height: 50px;
            padding: 15px 15px;
            font-size: 18px;
            line-height: 20px;
            text-decoration: none;
            color: orangered;
            font-family: cursive;
            font-weight: 700;

        }

      

        nav.main-navigation ul.nav-list {
            margin: 0;
            padding: 0;
            list-style: none;
            position: relative;
            text-align: right;
        }

        .nav-list li.nav-list-item {
            display: inline-block;
            line-height: 40px;
            margin-left: 30px;
            margin-top: 15px;
        }

        a.test {
            
            font-size: 30px;
            font-family: sans-serif;
            font-weight: normal;
            cursor: pointer;
            position: relative;
            color: #404040;
        }
	</style>
	 <nav class="movies_nav" >
       
        <ul class="nav-list navbar-center">
            <li class="nav-list-item">
                <a asp-area="" asp-controller="Home" asp-action="Index" class="active nav-link test">Home</a>
            </li>
            <li class="nav-list-item">
                <a asp-area=""  asp-controller="Home" asp-action="Contact" class="nav-link test">Contact</a>
            </li>
            <li class="nav-list-item">
                <a asp-area="" asp-controller="Home" asp-action="Faq" class="nav-link test">Faq</a>
            </li>
				@if (SignInManager.IsSignedIn(User))
								{
            <li class="nav-list-item">
                <a asp-area="Identity" asp-page="/Account/Manage/Index" class="nav-link test">Profile</a>
            </li>
			<li class="nav-list-item">
				<form id="logoutForm" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
					<button id="logout" class="btn btn-warning btn-xs"  type="submit"><i class="fas fa-sign-out-alt"></i></button>

				</form>
			</li>
								}		
            
        </ul>

    </nav>
<!-- //nav -->


<!-- general -->
	<div class="general">
		@RenderBody()
		
	</div>
<!-- //general -->
